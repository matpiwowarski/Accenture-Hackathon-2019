            </Button>
            <Button x:Name="MichalButton" HorizontalAlignment="Center" VerticalAlignment="Center" Width="64" Margin="0,0,0, 50" Height="64" BorderBrush="{x:Null}" Foreground="#E5FFFFFF" Background="#19FFFFFF" Click="MichalButton_Click">
