            <TextBox x:Name="TextBox" HorizontalAlignment="Center" Height="30" Margin="0,0,0,20" TextWrapping="Wrap" VerticalAlignment="Bottom" Width="773" FontSize="24" Background="#19000000" Foreground="#E5000000" TextChanged="TextBox_TextChanged">
            </TextBox>
            <Button x:Name="Send_Button" HorizontalAlignment="Center" Margin="0,550,0,60" VerticalAlignment="Bottom" Width="64" Height="64" Click="Send_Button_Click" IsDefault="True">
                <Image x:Name="Logo_Image" Source="source/icons/logo.png" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Button>
            <Border x:Name="UserImage1Border" BorderThickness="1" HorizontalAlignment="Left" Height="175" Margin="40,70,0,0" VerticalAlignment="Top" Width="350" BorderBrush="#E5000000">
                <Image x:Name="UserImage1"/>
            </Border>
            <Border x:Name="UserImage2Border" BorderThickness="1"  HorizontalAlignment="Left" Height="175" Margin="40,275,0,0" VerticalAlignment="Top" Width="350" BorderBrush="#E5000000">
