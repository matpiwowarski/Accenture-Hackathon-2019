            <Border x:Name="ContactImage3Border" HorizontalAlignment="Right" Height="180" Margin="0,460,40,0" VerticalAlignment="Top" Width="350" BorderBrush="#E5000000">
                <Image x:Name="ContactImage3"/>
            </Border>

        </Grid>
        <Grid x:Name="User_Grid" HorizontalAlignment="Left" Height="737" VerticalAlignment="Top" Width="158">
            <TextBox x:Name="Username_Label" Text="username" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Background="{x:Null}" Foreground="#E5FFFFFF" FontSize="24" TextChanged="Username_Label_TextChanged"/>
