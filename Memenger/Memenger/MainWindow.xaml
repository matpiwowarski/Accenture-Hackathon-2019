                <Image x:Name="ContactImage2"/>
            </Border>
            <Border x:Name="ContactImage3Border" BorderThickness="1"  HorizontalAlignment="Right" Height="175" Margin="0,480,40,0" VerticalAlignment="Top" Width="350" BorderBrush="#E5000000">
                <Image x:Name="ContactImage3"/>
            </Border>
            <Label x:Name="UserDate1" Content="" HorizontalAlignment="Left" Margin="336,244,0,0" VerticalAlignment="Top"/>
            <Label x:Name="UserDate2" Content="" HorizontalAlignment="Left" Margin="336,449,0,0" VerticalAlignment="Top"/>
            <Label x:Name="UserDate3" Content="" HorizontalAlignment="Left" Margin="336,655,0,0" VerticalAlignment="Top"/>
            <Label x:Name="ContactDate1" Content="" HorizontalAlignment="Left" Margin="759,245,0,0" VerticalAlignment="Top" RenderTransformOrigin="5.759,0.769"/>
            <Label x:Name="ContactDate2" Content="" HorizontalAlignment="Left" Margin="759,449,0,0" VerticalAlignment="Top"/>
            <Label x:Name="ContactDate3" Content="" HorizontalAlignment="Left" Margin="759,655,0,0" VerticalAlignment="Top"/>

