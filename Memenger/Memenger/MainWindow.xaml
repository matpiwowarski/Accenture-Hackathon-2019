            <TextBox x:Name="ContactNameLabel" Text="contact name" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="36" Margin="0,10,0,0" Background="{x:Null}" Foreground="#E5000000" TextChanged="ContactNameLabel_TextChanged"/>
            <TextBox x:Name="TextBox" HorizontalAlignment="Center" Height="30" Margin="0,0,0,20" TextWrapping="Wrap" VerticalAlignment="Bottom" Width="773" FontSize="24" Background="#19000000" Foreground="#E5000000" TextChanged="TextBox_TextChanged">
            </TextBox>
            <Button x:Name="Send_Button" HorizontalAlignment="Center" Margin="0,550,0,0" VerticalAlignment="Center" Width="64" Height="64" Click="Send_Button_Click" IsDefault="True">
                <Image x:Name="Logo_Image" Source="source/icons/logo.png" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Button>
            <Border x:Name="UserImage1Border" HorizontalAlignment="Left" Height="180" Margin="40,80,0,0" VerticalAlignment="Top" Width="350" BorderBrush="#E5000000">
                <Image x:Name="UserImage1"/>
            </Border>
            <Border x:Name="UserImage2Border" HorizontalAlignment="Left" Height="180" Margin="40,270,0,0" VerticalAlignment="Top" Width="350" BorderBrush="#E5000000">
